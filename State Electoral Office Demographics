## Load in data
dataset3 <- read.delim(file.choose())

##PRIMPT:
#explanation of what analysis entails
#how many of them could we match analysis to of the whole set
# how many nulls do we have
## Isolate by different Tiers, which are different levels of office   

## Look at Michigan data
michigan <- subset(dataset3, POSITION_STATE == "MI")
#Give breakdowns per level
######State Level
        table(michigan$RACE)
        table(michigan$GENDER)
        table(michigan$AGE)
        table(michigan$MARITAL_STATUS)
        table(michigan$INCOME_MODEL)
        table(michigan$CATALISTSYNTHETIC_URBANITY_MODEL)
        mean(michigan$IDEOLOGY_MODEL, na.rm =TRUE)
        median(michigan$IDEOLOGY_MODEL, na.rm =TRUE)
      
   
   ##Tier 1/name those significant individuals     
        michigan1 <- subset(dataset3, subset = (POSITION_STATE == "MI") & (TIER == "1"))
              table(michigan1$RACE)
              table(michigan1$GENDER)
              table(michigan1$AGE)
              table(michigan1$MARITAL_STATUS)
              table(michigan1$INCOME_MODEL)
              table(michigan1$CATALISTSYNTHETIC_URBANITY_MODEL)
              mean(michigan1$IDEOLOGY_MODEL, na.rm =TRUE)
              median(michigan1$IDEOLOGY_MODEL, na.rm =TRUE)
        
  ##Tier 2
        michigan2 <- subset(dataset3, subset = (POSITION_STATE == "MI") & (TIER == "2"))
              table(michigan2$RACE)
              table(michigan2$GENDER)
              table(michigan2$AGE)
              table(michigan2$MARITAL_STATUS)
              table(michigan2$INCOME_MODEL)
              table(michigan2$CATALISTSYNTHETIC_URBANITY_MODEL)
              mean(michigan2$IDEOLOGY_MODEL, na.rm =TRUE)
              median(michigan2$IDEOLOGY_MODEL, na.rm =TRUE)
              
      
  ##Tier 3
        michigan3 <- subset(dataset3, subset = (POSITION_STATE == "MI") & (TIER == "3"))
              table(michigan3$RACE)
              table(michigan3$GENDER)
              table(michigan3$AGE)
              table(michigan3$MARITAL_STATUS)
              table(michigan3$INCOME_MODEL)
              table(michigan3$CATALISTSYNTHETIC_URBANITY_MODEL)
              mean(michigan3$IDEOLOGY_MODEL, na.rm =TRUE)
              median(michigan3$IDEOLOGY_MODEL, na.rm =TRUE)
              

  ##Tier 4
        michigan4 <- subset(dataset3, subset = (POSITION_STATE == "MI") & (TIER == "4"))
              table(michigan4$RACE)
              table(michigan4$GENDER)
              table(michigan4$AGE)
              table(michigan4$MARITAL_STATUS)
              table(michigan4$INCOME_MODEL)
              table(michigan4$CATALISTSYNTHETIC_URBANITY_MODEL)
              mean(michigan4$IDEOLOGY_MODEL, na.rm =TRUE)
              median(michigan4$IDEOLOGY_MODEL, na.rm =TRUE)
              
        

